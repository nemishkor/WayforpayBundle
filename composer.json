{
    "name": "nemishkor/wayforpay",
    "type": "symfony-bundle",
    "license": "GNU v.3.0",
    "require": {
        "php": "^7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Nemishkor\\Wayforpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nemishkor\\Wayforpay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.0"
        }
    }
}
